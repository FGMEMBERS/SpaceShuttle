			<sum>
				<property>systems/rms/ellbow-y</property>
				<product>
					<value>6.25</value>
					<cos><property>systems/rms/sum-ellbow-pitch-rad</property></cos>
					<sin><property>systems/rms/ang-shoulder-yaw-rad</property></sin>
				</product>
			</sum>
		</function>
