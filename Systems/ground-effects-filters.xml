<PropertyList>

	<filter>
        <name>Left Tail Ground Effect</name>
        <type>gain</type>
        <enable>
            <condition>
                <not>
                    <property>/sim/freeze/replay-state</property>
                </not>
            </condition>
        </enable>
        <input>
            <property>/fdm/jsbsim/ground/left-tail-ground-effect-speed-kt</property>
        </input>
        <output>
            <property>/fdm/jsbsim/ground/left-tail-ground-effect-speed-kt-actual</property>
        </output>
    </filter>
	
