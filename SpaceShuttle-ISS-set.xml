<?xml version="1.0"?>

<!-- this puts the Shuttle into a high orbit -->

<PropertyList include="SpaceShuttle-set.xml">
<sim>
  <description>NASA Space Shuttle Atlantis approaching ISS</description>
  <variant-of>SpaceShuttle</variant-of>
  <presets>
	<altitude-ft>1500000.0</altitude-ft>
	<stage type="int">6</stage>
  </presets>
  <startup>
    <splash-texture>Aircraft/SpaceShuttle/SpaceShuttle-orbit-splash.png</splash-texture>
  </startup>
</sim>

<fdm>
   <jsbsim>
	<systems>
	   <dps>
		<ops type="int">2</ops>
		<major-mode type="int">201</major-mode>
	   </dps>
	</systems>
   </jsbsim>
</fdm>



</PropertyList>
