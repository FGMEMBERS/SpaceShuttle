<?xml version="1.0"?>

<!-- this puts the Shuttle into a high orbit -->

<PropertyList include="SpaceShuttle-common.xml">
<sim>
  <description>NASA Space Shuttle Atlantis approaching ISS</description>
  <variant-of>SpaceShuttle-launch</variant-of>
  <presets>
	<altitude-ft>1500000.0</altitude-ft>
	<stage type="int">6</stage>
  </presets>
  <!--<startup>
    <splash-texture>Aircraft/SpaceShuttle/SpaceShuttle-orbit-splash.png</splash-texture>
  </startup>-->
 <previews>
      <preview>
        <type>exterior</type>
        <splash type="bool">true</splash>
        <path>Pics/iss-splash01.png</path>
      </preview>
  </previews>
  <config>
    <shuttle>
      <prelaunch-flag type="int">0</prelaunch-flag>
    </shuttle>
  </config>
</sim>

<fdm>
   <jsbsim>
	<systems>
	   <dps>
		<ops type="int">2</ops>
		<major-mode type="int">201</major-mode>
	   </dps>
	</systems>
   </jsbsim>
</fdm>



</PropertyList>
