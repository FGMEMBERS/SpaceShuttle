 	<row>1</row>
        <col>2</col>
          <property>/fdm/jsbsim/systems/rcs-hardware/heater-left-A-status</property>
          <label>left pod A</label>
          <live>true</live>
 	  <binding>
              <command>dialog-apply</command>
              <object-name>left-A</object-name>
          </binding>
      </checkbox>

    <checkbox>
        <name>left-B</name>
 	<row>1</row>
