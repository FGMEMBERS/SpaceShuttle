<?xml version="1.0"?>

<PropertyList>
	<name>rcs_oms_thermal</name>
	<layout>vbox</layout>
	<default-padding>1</default-padding>
	<modal>false</modal>
	<width>600</width>

	<empty>
		<pref-height>6</pref-height>
	</empty>

	<text>
		<label>RCS and OMS thermal control</label>
	</text>

	<empty>
		<pref-height>4</pref-height>
	</empty>

	<hrule/>


 <group>
    <layout>table</layout>
 	 

   <text>
      <label>RCS system heaters</label>
      <halign>center</halign>
      <row>0</row>
      <col>0</col>
    </text>

    <checkbox>
        <name>fwd-A</name>
 	<row>1</row>
        <col>0</col>
          <property>/fdm/jsbsim/systems/rcs-hardware/heater-fwd-A-status</property>
          <label>fwd A</label>
          <live>true</live>
 	  <binding>
              <command>dialog-apply</command>
              <object-name>fwd-A</object-name>
          </binding>
    </checkbox>

    <checkbox>
        <name>fwd-B</name>
 	<row>1</row>
        <col>1</col>
          <property>/fdm/jsbsim/systems/rcs-hardware/heater-fwd-B-status</property>
