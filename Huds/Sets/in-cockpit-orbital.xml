		<x>380</x>
		<y>50</y>

		<halign>center</halign>
		<input>
			<property>/position/longitude-string</property>
		</input>
	</label>

	<label>
		<name>G Load</name>
		<condition>
				<equals>
				<property>/sim/current-view/view-number</property>
				<value>0</value>
				</equals>
		</condition>
		<x>10</x>
		<y>-60</y>
		<width>28</width>
		<height>12</height>

		<format>%4.2f g</format>
		<halign>right</halign>
		<box>false</box>

		<input>
			<property>/fdm/jsbsim/accelerations/a-pilot-ft_sec2</property>
			<factor>0.03108095</factor>
			<damp>1.3</damp>
		</input>

		<blinking>
			<interval>0.25</interval>
			<condition>
				<or>
					<!-- G load > 3.0 -->
					<greater-than>
						<property>/fdm/jsbsim/accelerations/a-pilot-ft_sec2</property>
						<value>96.52215</value>
					</greater-than>

					<!-- G load < -1.0 -->
					<less-than>
						<property>/fdm/jsbsim/accelerations/a-pilot-ft_sec2</property>
						<value>-31.17405</value>
					</less-than>
				</or>
			</condition>
		</blinking>
	</label>

