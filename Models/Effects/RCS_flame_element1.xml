<PropertyList>

  <path>RCS_flame1.ac</path>


  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>Light1</object-name>
  
  </effect>

  <animation>
    <type>alpha-test</type>
    <alpha-factor>0.01</alpha-factor>
  </animation>

  <animation>
    <type>billboard</type>
    <object-name>Light1</object-name>
    <spherical type="bool">true</spherical>
  </animation>


  
  <animation>
    <type>material</type>
    <object-name>Light1</object-name>
    <emission>
      <red>   0.5 </red>
      <green> 0.7 </green>
      <blue>  0.92 </blue>
    </emission>
  </animation>

 

</PropertyList>
