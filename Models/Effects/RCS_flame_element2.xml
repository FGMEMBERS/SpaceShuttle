<PropertyList>

  <path>RCS_flame2.ac</path>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>Light2</object-name>
  </effect>

  <animation>
    <type>alpha-test</type>
    <alpha-factor>0.01</alpha-factor>
  </animation>

  <animation>
    <type>billboard</type>
    <object-name>Light2</object-name>
    <spherical type="bool">true</spherical>
  </animation>


  
  <animation>
    <type>material</type>
    <object-name>Light2</object-name>
    <emission>
      <red>   0.55 </red>
      <green> 0.6 </green>
      <blue>  0.93 </blue>
    </emission>
  </animation>

  
</PropertyList>
