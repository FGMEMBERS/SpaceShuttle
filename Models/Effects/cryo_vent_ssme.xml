<?xml version="1.0"?>


<PropertyList>

<particlesystem>
	 <name>cryo-vent-ssme</name>
	 <texture>smoke.png</texture>
	 <emissive>true</emissive>
	 <lighting>false</lighting>
	 <x-m>0.0</x-m>
	 <y-m>0.0</y-m>
	 <z-m>0.0</z-m>
	 <roll-deg>0.0</roll-deg>
	 <pitch-deg>0.0</pitch-deg>
	 <heading-deg>0.0</heading-deg>
 
	 <attach>local</attach>
	 
	 <placer>
		 <type>point</type>
	 </placer>
	 
	 <shooter>
		 <theta-min-deg>85</theta-min-deg>
		 <theta-max-deg>115</theta-max-deg>
		 <phi-min-deg>-80</phi-min-deg>
		 <phi-max-deg>-100</phi-max-deg>
		<speed-mps>
		<value>0</value>
		<spread>0.5</spread>
		</speed-mps>
		 <rotation-speed>
			<x-min-deg-sec>1</x-min-deg-sec>
			<y-min-deg-sec>1</y-min-deg-sec>
			<z-min-deg-sec>1</z-min-deg-sec>
			<x-max-deg-sec>5</x-max-deg-sec>
			<y-max-deg-sec>5</y-max-deg-sec>
			<z-max-deg-sec>5</z-max-deg-sec>
		 </rotation-speed>
	 </shooter>
	 
	 <counter>
		 <particles-per-sec>
			 <value>60</value>
			 <spread>0</spread>
		 </particles-per-sec>
	 </counter>
	 
	 <align>billboard</align>
	 
	 <particle>
		 <start>
			 <color>
				<red>
					<property>/rendering/scene/diffuse/blue</property>
				</red>
				<green>
					<property>/rendering/scene/diffuse/blue</property>
				</green>
				<blue>
					<property>/rendering/scene/diffuse/blue</property>
				</blue>
				<alpha>
					<value>0.25</value>
				</alpha>
			 </color>
			 <size>
				 <value>0.15</value>
			 </size>
		 </start>
		 <end>
			 <color>
				 <red>
					<property>/rendering/scene/diffuse/blue</property>
				</red>
				<green>
					<property>/rendering/scene/diffuse/blue</property>
				</green>
				<blue>
					<property>/rendering/scene/diffuse/blue</property>
				</blue>
				<alpha>
					<value>0.0</value>
				</alpha>
			 </color>
			 <size>
				 <value>0.85</value>
			 </size>
		 </end>
		 <life-sec>
			 <value>1.0</value>
		 </life-sec>
		 <mass-kg>2.5</mass-kg>
		 <radius-m>0.50</radius-m>
	 </particle>
	 
	 <program>
		 <fluid>air</fluid>
		 <gravity type="bool">true</gravity>
		 <wind type="bool">true</wind>
	 </program>
	 
</particlesystem>

</PropertyList>
