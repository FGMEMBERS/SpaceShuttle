    <!--Aft Left RCS HE Oxid "Cover" -->
    <animation>
        <type>knob</type>
        <object-name>aft-left-rcs-he-oxid-cvr</object-name>
        <property>/sim/model/panel/cover/aft-left-rcs-he-oxid-cvr</property>
        <factor>75</factor>
        <offset-deg>0</offset-deg>
        <center>
            <x-m>-11.8864</x-m>
            <y-m> -0.1084</y-m>
            <z-m>  0.3235</z-m>
        </center>
        <axis>
            <x> 1   </x>
            <y> -.13</y>
            <z>  .13</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/sim/model/panel/cover/aft-left-rcs-he-oxid-cvr</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
            <binding>
                <command>nasal</command>
                <script>SpaceShuttle.click("avionics")</script>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>aft-left-rcs-he-oxid-cvr</tooltip-id>
                <label>Remove Cover</label>
                <property>/sim/model/panel/cover/aft-left-rcs-he-oxid-cvr</property>
            </binding>
        </hovered>
    </animation>

