    <!-- Left RCS HE Oxid "Switch" -->
    <animation>
        <type>knob</type>
        <object-name>left-rcs-he-oxid</object-name>
        <property>/fdm/jsbsim/systems/rcs-hardware/helium-left-rcs-valve-A-status</property>
        <factor>-75</factor>
        <offset-deg>50</offset-deg>
        <center>
            <x-m>-11.8837</x-m>
            <y-m> -0.1255</y-m>
            <z-m>  0.3135</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/fdm/jsbsim/systems/rcs-hardware/helium-left-rcs-valve-A-status</property>
                <factor>1</factor>
                <!-- This should be a 3 stage swtch, GPC is not programed so it is bypassed -->
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
            <binding>
                <command>nasal</command>
                <script>SpaceShuttle.click("avionics")</script>
            </binding>
        </action>
        <hovered>
          <binding>
                <command>set-tooltip</command>
                <tooltip-id>left-rcs-he-oxid</tooltip-id>
                <label>Left RCS HE Oxid: %s</label>
                <property>/fdm/jsbsim/systems/rcs-hardware/helium-left-rcs-valve-A-status</property>
                <mapping>nasal</mapping>
                <script>
                    var modes = ['Close', 'Open'];
                    #var modes = ['Close', 'GPC', 'Open'];
                    return modes[arg[0]];
                </script>
          </binding>
        </hovered>
    </animation>

    <!-- Left RCS HE Oxid "Talkback" -->
    <animation>
        <type>select</type>
        <object-name>left-rcs-he-oxid-cl</object-name>
        <condition>
            <equals>
                <property>/fdm/jsbsim/systems/rcs-hardware/helium-left-rcs-valve-A-status</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    <animation>
