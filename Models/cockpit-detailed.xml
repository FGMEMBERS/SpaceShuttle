<PropertyList>
    <path>cockpit-detailed.ac</path>
    <nopreview/>

    <!-- grain -->
    <!--effect>
        <inherits-from>Aircraft/SpaceShuttle/Models/Effects/Interior/velcro</inherits-from>
    </effect-->
    <effect>
        <inherits-from>Aircraft/SpaceShuttle/Models/Effects/Interior/framing-detailed</inherits-from>
        <object-name>aft_base_set</object-name>
        <object-name>roof_assembly</object-name>
    </effect>

    <!-- implicit lighting -->
    <effect>
        <inherits-from>Aircraft/SpaceShuttle/Models/Effects/Interior/shuttle-interior-detailed</inherits-from>
        <!-- cabin -->
        <object-name>aft_equipment_group</object-name>
         <!-- Panels -->
        <object-name>O1-panel</object-name>
        <object-name>O2-panel</object-name>
        <object-name>O3-panel</object-name>
        <!-- misc -->
        <object-name>egress-system</object-name>
    </effect>

</PropertyList>
