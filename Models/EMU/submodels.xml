<PropertyList>

  <submodel> 
    <name>satelit</name>
    <model>Aircraft/SpaceShuttle/Models/EMU/satelit.xml</model>
    
    <trigger>sim/multiplay/generic/int[9]</trigger>
    <speed> 0.0 </speed>
    <repeat> true </repeat>
    <delay> 0 </delay>
    <count> 50 </count>
    <x-offset>  24 </x-offset>
    <y-offset>  0 </y-offset>
    <z-offset>  38 </z-offset>
    <yaw-offset> 0 </yaw-offset>
    <pitch-offset> 90 </pitch-offset>
    <cd> 0.65 </cd>
    <eda> 100 </eda>
    <weight> 1200 </weight>
    <wind> true </wind>
    <buoyancy> 32 </buoyancy>
    <life> 600 </life>
    <randomness>1.0</randomness>
  </submodel>

<submodel>
  <name>SRB-submodel</name>
  <model>Aircraft/SpaceShuttle/Models/srb-falling.xml</model>
  <trigger>/controls/shuttle/SRB-drop</trigger>
  <repeat>true</repeat>
  <count>2</count>
  <speed>0</speed>
  <wind>false</wind>
  <aero-stabilised>true</aero-stabilised>
  <eda>1.42</eda>
  <cd>0.75</cd>
  <weight>500</weight>
  <x-offset>-5.2</x-offset>
  <y-offset>-6.35</y-offset>
  <z-offset>-10.6</z-offset>
  <pitch-offset>0</pitch-offset>
  <yaw-offset>0</yaw-offset>
  <life>900</life>
  <delay>0</delay>
  <impact>false</impact>
</submodel>

</PropertyList>
