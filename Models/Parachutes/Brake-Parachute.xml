<PropertyList>

    <!-- Brake Parachute -->

	<path>Main-Parachute.ac</path>
	<nopreview/>

	<offsets>
		<x-m>0.0</x-m>
		<y-m>0.0</y-m>
		<z-m>0.0</z-m>
		<pitch-deg>0</pitch-deg>
	</offsets>

    <animation>
        <type>scale</type>
        <property>/controls/shuttle/drag-chute-deploy-timer</property>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>.09</dep>
            </entry>
            <entry>
                <ind>2</ind>
                <dep>.09</dep>
            </entry>
            <entry>
                <ind>3</ind>
                <dep>.17</dep>
            </entry>
            <entry>
                <ind>6</ind>
                <dep>.17</dep>
            </entry>
            <entry>
                <ind>7</ind>
                <dep>.35</dep>
            </entry>
        </interpolation>
        <center>
            <x-m> 0.0</x-m>
            <y-m> 0.0</y-m>
            <z-m> 0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>1</z>
        </axis>
    </animation>

	<animation>
		<type>spin</type>
		<property>sim/multiplay/generic/float[0]</property>
		<factor>0.15</factor>
		<center>
			<x-m>0.0</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>

    <animation>
        <type>rotate</type>
        <property>orientation/pitch-deg</property>
        <factor>-1.0</factor>

        <center>
            <x-m> 0.0</x-m>
            <y-m> 0.0</y-m>
            <z-m> 0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>
