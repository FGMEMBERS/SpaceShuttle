<PropertyList>
	<path>shuttle_o2.ac</path>

   
   <!-- heat shield glow -->

  <animation>
    	<type>material</type>
   	<object-name>heatshield</object-name>	
   	<emission>
      		<factor-prop>/fdm/jsbsim/systems/thermal/entry-flame-alpha</factor-prop>
      		<red>   1.0 </red>
      		<green> 0.2 </green>
		<blue>  0.1 </blue>
    	</emission>
  </animation>



 



    <!-- ******************************************************************* -->
    <!-- Cockpit                                                             -->
    <!-- ******************************************************************* -->
    <model>
        <path>cockpit.xml</path>
        <offsets>
            <x-m> 0 </x-m>
            <y-m> 0 </y-m>
            <z-m> 0 </z-m>
        </offsets>
    </model>

  <!-- SSME Motors Placements -->

  <model>
    <path>Aircraft/SpaceShuttle/Models/SSME1.xml</path>
  </model>

  <model>
    <path>Aircraft/SpaceShuttle/Models/SSME2.xml</path>
    <offsets>
      <x-m> 0.0 </x-m>
      <y-m> -1.5 </y-m>
      <z-m> -2.5 </z-m>
    </offsets>
  </model>

  <model>
    <path>Aircraft/SpaceShuttle/Models/SSME3.xml</path>
    <offsets>
      <x-m> 0.0 </x-m>
      <y-m> 1.5 </y-m>
      <z-m> -2.5 </z-m>
    </offsets>
  </model>

<!-- Enterprise air data boom -->

    
  <model>
    <path>Aircraft/SpaceShuttle/Models/air-data-boom.ac</path>
	<condition>
		<equals>
			<property>/sim/config/shuttle/approach-and-landing-test-config</property>
			<value>1</value>
		</equals>
 	</condition>
  </model>

<!-- Enterprise tail cone -->

  <model>
    <path>Aircraft/SpaceShuttle/Models/TailCone.ac</path>
	<condition>
		<equals>
			<property>/sim/config/shuttle/approach-and-landing-test-config</property>
			<value>1</value>
		</equals>
 	</condition>
  </model>

<!-- OMS thruster exhaust and rear RCS modules -->

 <model>
    <path>Aircraft/SpaceShuttle/Models/OMSPods.xml</path>
  </model>



<!-- airlock module -->
<model>
    <path>Aircraft/SpaceShuttle/Models/PayloadBay/airlock_module.ac</path>
     <offsets>
       <x-m> 0.0 </x-m>
       <y-m> 0.0 </y-m>
       <z-m> -0.0 </z-m>
       <heading-deg>0.0</heading-deg>
     </offsets>
</model>

<!-- landing gear -->
<model>
  <path>Aircraft/SpaceShuttle/Models/LandingGears.ac</path>
</model>

<!-- landing gear damaged-->
<model>
	<path>Aircraft/SpaceShuttle/Models/damage.ac</path>
</model>

 <!-- front gear undamaged -->
 <animation>
   <type>select</type>
   <object-name>Cube</object-name>
   <object-name>Shock</object-name>
   <object-name>Axel</object-name>
   <object-name>Rim</object-name>
   <object-name>Tire</object-name>
   <condition>
		<and>
			<equals>
				<property>/fdm/jsbsim/gear/gear-pos-norm</property>
				<value>1</value>
			</equals>
			<equals>
				<property>/controls/shuttle/ET-static-model</property>
				<value>0</value>
			</equals>
			<equals>
				<property>/fdm/jsbsim/systems/failures/gearstrut-nose-condition</property>
				<value>1</value>
			</equals>
		</and>
	</condition>
 </animation>

