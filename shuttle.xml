	<product>
        	<property>aero/qbar-psf</property>
         	<property>metrics/Sw-sqft</property>
    		<sum>
			<value>-0.053</value>
			<product>
				<value>2.73</value>
				<property>aero/alpha-rad</property>			
			</product>
			<product>
				<value>-1.55</value>
				<pow>
					<property>aero/alpha-rad</property>
					<value>2.0</value>
				</pow>
			</product>
		</sum>
		<pow>
		<property>aero/coefficient/K</property>
		<sum>
			<value>-1.01</value>
			<product>
				<property>aero/alpha-rad</property>
				<value>1.1</value>
			</product>
		</sum>
		</pow>
	<property>systems/failures/aero-structure-condition</property>
	</product> 
	</function>   
  </axis>

