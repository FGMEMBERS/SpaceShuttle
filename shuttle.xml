        	<property>aero/qbar-psf</property>
         	<property>metrics/Sw-sqft</property>
    		<sum>
			<value>0.01</value>
			<product>
				<value>0.028</value>
				<pow>
					<property>velocities/mach</property>
					<value>1.4</value>
				</pow>			
			</product>
			<product>
				<value>1.79</value>
				<pow>
					<property>aero/alpha-rad</property>
					<value>2.0</value>
				</pow>
			</product>
		</sum>
		<pow>
		<property>aero/coefficient/K</property>
		<sum>
			<value>-1.4</value>
			<product>
				<property>aero/alpha-rad</property>
				<value>1.5</value>
			</product>
		</sum>
		</pow>
	</product> 
